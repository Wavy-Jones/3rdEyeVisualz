# ========================================
# 3rdEye Visualz - Environment Configuration
# ========================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# ========================================
# API Configuration
# ========================================
# Your backend API URL
ENV_API_URL=https://api.3rdeyevisualz.com

# API Key for authentication (KEEP THIS SECRET!)
# Generate a secure key: https://www.uuidgenerator.net/
ENV_API_KEY=your-secure-api-key-here

# ========================================
# Google Analytics
# ========================================
# Google Analytics 4 Measurement ID
# Get yours from: https://analytics.google.com/
ENV_GA_ID=G-XXXXXXXXXX

# ========================================
# reCAPTCHA Configuration
# ========================================
# Google reCAPTCHA v3 Site Key
# Get yours from: https://www.google.com/recaptcha/admin
ENV_RECAPTCHA_SITE_KEY=6LeIxAcTAAAAAJcZVRqyHh71UMIEGNQ_MXjiZKhI

# ========================================
# Business Information
# ========================================
# Optional: Override default business info
# BUSINESS_EMAIL=infntmediasolutions@gmail.com
# BUSINESS_PHONE=+27721480697
# BUSINESS_NAME=3rdEye Visualz

# ========================================
# Feature Flags
# ========================================
# Enable/disable features
# ENABLE_ANALYTICS=true
# ENABLE_RECAPTCHA=false
# ENABLE_BOOKING=true
# ENABLE_CONTACT_FORM=true

# ========================================
# Development vs Production
# ========================================
# Set to 'production' when deploying
NODE_ENV=development
